{
  "manifest_version": 3,
  "name": "LinkedIn AdBlocker",
  "version": "0.0.2",
  "description": "Blocks 'Promoted' posts on LinkedIn.",
  "author": "<AUTHOR NAME>",
  "permissions": ["scripting", "tabs", "webNavigation"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "background": {
    "service_worker": "extension.js"
  },
  "action": {
    "default_title": "LinkedIn AdBlocker"
  }
}
